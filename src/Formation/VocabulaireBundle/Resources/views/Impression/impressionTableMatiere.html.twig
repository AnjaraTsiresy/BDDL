<meta http-equiv="content-type" content="text/html; charset=utf-8"/>
<link rel="stylesheet" href="{{ pathToWeb }}/css/pdf1.css" type="text/css">
<style>
    .A4{
        background: white;
        height: 21cm;
        width: 29.7cm;
        display: block;
        margin: 0 auto;
        margin-top: 0.5cm;
    }

    @media print {
        body, .A4 {
            margin: 0;
            box-shadow: 0;
        }
    }
</style>

{% for i, dataTable in tablenbreParTable %}
    <div class="A4">
        <div class='conteneur'>
            <div class='colonne1 tableMatiere'>
                <p style='margin-top:50px;font-size: 12pt;color: blue; font-style : italic;'>TABLE DES MATIERES</p><br/><br/>

                {% if i==0 %}
                    <b>{{ nom_societe }}</b>
                    <br/><br/>
                {% endif %}
                {% for table in table_matiere %}
                    {% if loop.index <= 8 and i  == 0 %}
                    <span style="float:left;">{{ table.theme }}</span><span class='pointpoint'>&nbsp; </span><span style='margin-right:60px;float:right;'>{{ table.nbPage }}</span></br>
                    <br/>
                    {% elseif loop.index >= (9 + (i-1)) and loop.index <= (8 * (i+1)) %}
                        <span style="float:left;">{{ table.theme }}</span><span class='pointpoint'>&nbsp; </span><span style='margin-right:60px;float:right;'>{{ table.nbPage }}</span></br>
                        <br/>
                    {% endif %}
                {% endfor %}
                <div class='nomprototype1'>{{ prototypeTitle }}</div>
            </div>
            <div class='colonne2 tableMatiere'>
                <p style='margin-top:50px;font-size: 12pt;color: blue; font-style : italic;'>TABLE DES MATIERES</p><br/><br/>

                {% if i==0 %}
                    <b>{{ nom_societe }}</b>
                    <br/><br/>
                {% endif %}
                {% for table in table_matiere %}
                    {% if loop.index <= 8 and i  == 0 %}
                        <span style="float:left;">{{ table.theme }}</span><span class='pointpoint'>&nbsp; </span><span style='margin-right:60px;float:right;'>{{ table.nbPage }}</span></br>
                        <br/>
                    {% elseif loop.index >= (9 + (i-1)) and loop.index <= (8 * (i+1)) %}
                        <span style="float:left;">{{ table.theme }}</span><span class='pointpoint'>&nbsp; </span><span style='margin-right:60px;float:right;'>{{ table.nbPage }}</span></br>
                        <br/>
                    {% endif %}
                {% endfor %}
                <div class='nomprototype2'>{{ prototypeTitle }}</div>
            </div>
        </div>
    </div>
{% endfor %}

