{% extends '::base.html.twig' %}

{% block body -%}
    <h1>nl_article</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Article</th>
                <td>{{ entity.article }}</td>
            </tr>
            <tr>
                <th>Monthnewsletter</th>
                <td>{{ entity.monthNewsLetter }}</td>
            </tr>
            <tr>
                <th>Origine</th>
                <td>{{ entity.origine }}</td>
            </tr>
            <tr>
                <th>Journaliste</th>
                <td>{{ entity.journaliste }}</td>
            </tr>
            <tr>
                <th>Texte</th>
                <td>{{ entity.texte }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ entity.date|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('articles') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('articles_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
